		Else
			aCols[nSeek,nP_VlUn]:=CO3->CO3_VLUNIT
		EndIf
		If	CO3->CO3_REVOGA=="1"
			aCols[nSeek,nP_Revo]:=CO3->CO3_REVOGA
		EndIf
		aCols[nSeek,Len(aHeader)+1] := .F. 

	Else
		If	(cAval=="2" .Or.  cAval=="3") .And.  CO3->CO3_REVOGA<>"1" .And.  (CO3->CO3_CLASS=="1" .Or.  CO3->CO3_CLAANT=="1")
