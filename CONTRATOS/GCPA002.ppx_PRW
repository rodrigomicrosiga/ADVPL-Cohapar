Function MetodoPreco(aCols)
Local aColsBkp 	:= aClone(aCols)
Local AcolsAux	:= {}
Local nX		:= 0
Local nY		:= 0
Local nQuant	:= 0
Local nSoma		:= 0
Local nPos	    := 0
Local nMenor	:= 0
Local nMaior	:= 0
Local nInterm	:= 0

For nX := 1 To Len(aCols)
	If !aTail(Acols[nX])
		AADD(AcolsAux, aCols[nX])
	EndIf
Next

If Len(AcolsAux) > 0
	Do Case
	Case nRadio==1
		For nY :=1 To Len (AcolsAux)
			nSoma := nSoma+AcolsAux[nY,4]
		Next
		nQuant	:= Len(AcolsAux)
		nPreco	:= nSoma/nQuant
	Case nRadio==2
		AcolsAux	:= ASort(AcolsAux,,,{|x,y|  x[4] < y[4] })
		nMenor		:= AcolsAux[1,4]
		nPos		:= Len(AcolsAux)
		nMaior		:= AcolsAux[nPos,4]
		nPreco		:= (nMenor+nMaior)/2
	Case nRadio==3
		AcolsAux	:= ASort(AcolsAux,,,{|x,y|  x[4] < y[4] })
		nPreco		:= AcolsAux[1,4]
	Case nRadio==4
		AcolsAux	:= ASort(AcolsAux,,,{|x,y|  x[4] < y[4] })
		nPos		:= Len(AcolsAux)
		nPreco		:= AcolsAux[nPos,4]
	Case nRadio==5
		AcolsAux	:= ASort(AcolsAux,,,{|x,y|  x[4] < y[4] })
		nPos		:= Len(AcolsAux)
		nInterm		:= round(nPos/2,0)
		nPreco		:= AcolsAux[nInterm,4]
	EndCase
EndIf

Return














