		EndIf
		If	CO3->CO3_REVOGA=="1"
			aCols[nSeek,nP_Revo]:=CO3->CO3_REVOGA
		EndIf
	EndIf

	(cAliasCO3)->(DbSkip())
EndDo
(cAliasCO3)->(DbCloseArea())

If	!Empty(aCols)
	ASort(aCols,,,{|x,y|x[nP_Rank] < y[nP_Rank]})
EndIf
RestArea(aAreaAnt)
Return













