ElseIf	cAval == "3"













__execSql(cAliasCO3," SELECT CO3.R_E_C_N_O_ RecCO3 FROM  "+RetSqlName('CO3')+" CO3 WHERE CO3.CO3_FILIAL =  '" +xFilial('CO3')+"'  AND CO3.CO3_CODEDT =  "+___SQLGetValue(CCODEDT)+" AND CO3.CO3_NUMPRO =  "+___SQLGetValue(CNUMPRO)+" AND CO3.D_E_L_E_T_= ' ' ORDER BY  "+ SqlOrder(CO3->(IndexKey())),{},.F.)

EndIf

While (cAliasCO3)->(!Eof())
	CO3->(DbGoTo((cAliasCO3)->RecCO3))

	If	(nSeek:=AScan(aCols,{|x| x[nP_ForCli]+x[nP_Loj]==CO3->CO3_CODIGO+CO3->CO3_LOJA}))==0

