! Descricao ! Tela de Cad. Tipos Doc. Fiscal - axCadastro								 !
! 			! 																 !
+----------------------------------------------------------------------------*/
User Function AxCadSZF()

	Local cAlias	:= "SZF"
	Local cTitulo	:= "Cadastro Tipo Doc. Fiscal"
	
	dbSelectArea(cAlias)
	(cAlias)->(dbSetOrder(1))
	
	AxCadastro(cAlias,cTitulo,"U_VldExSZF()","U_VldAlSZF()")

Return Nil

