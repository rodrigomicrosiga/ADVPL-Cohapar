User Function AxCadSZJ()

	Local cAlias	:= "SZJ"
	Local cTitulo	:= "Cadastro Quadro Deliberativo"
	
	dbSelectArea(cAlias)
	(cAlias)->(dbSetOrder(1))
	
	AxCadastro(cAlias,cTitulo,"U_VldExSZJ()","U_VldAlSZJ()")

Return Nil

