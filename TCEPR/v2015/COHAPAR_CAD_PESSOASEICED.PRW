User Function AxCadSZI()

	Local cAlias	:= "SZI"
	Local cTitulo	:= "Cadastro PessoaSEICED"
	
	dbSelectArea(cAlias)
	(cAlias)->(dbSetOrder(1))
	
	AxCadastro(cAlias,cTitulo,"U_VldExSZI()","U_VldAlSZI()")

Return Nil

